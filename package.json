{
  "name": "connext-network-explorer",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "start": "next start",
    "build": "next build && next export",
    "deploy": "aws s3 sync out/ s3://explorer.connext.network --acl public-read",
    "dev-dev": "env-cmd -f .env.staging next dev",
    "build-dev": "env-cmd -f .env.staging next build && next export",
    "deploy-dev": "aws s3 sync out/ s3://staging.explorer.connext.network --acl public-read",
    "dev-testnet": "NODE_ENV=test next dev",
    "build-testnet": "NODE_ENV=test next build && next export",
    "deploy-testnet": "aws s3 sync out/ s3://testnet.explorer.connext.network --acl public-read"
  },
